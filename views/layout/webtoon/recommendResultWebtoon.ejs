<br><br><br><br>
<h4>추천된 결과</h4>
<div class="row">
    <% for(var i=0; i<webtoons.length; i++) { %>
        <% if(webtoons[i].webtoon) { %>
        <div class="col-sm-6 col-md-4 col-lg-3 mt-4">
            <div class="card">
                <img class="card-img-top" src="<%= webtoons[i].webtoon.img_src %>">
                <div class="card-block">
                    <h4 class="card-title"><%= webtoons[i].webtoon.name %></h4>
                    <div class="meta">
                        <a href="#"><%= webtoons[i].webtoon.writer %></a>
                    </div>
                    <div class="card-text">
                        유사한 사용자 : <%= webtoons[i].userId %>
                    </div>
                </div>
                <div class="card-footer">
                </div>
            </div>
        </div>
        <% } %>
    <% } %>
</div>
